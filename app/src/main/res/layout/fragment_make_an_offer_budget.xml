                                    android:textSize="@dimen/_12sp" />

                                <com.jobtick.TextView.TextViewSemiBold
                                    android:id="@+id/txt_current_service_fee"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="10%"
                                    android:textColor="@color/textColor"
                                    android:textSize="@dimen/_16sp" />


                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_10sp"
                                android:layout_marginTop="@dimen/_10sp"
                                android:layout_marginEnd="@dimen/_10sp"
                                android:orientation="vertical">

                                <com.jobtick.TextView.TextViewMedium
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Complete more Jobs in-order to upgrade your account and be eligible to pay minimum service fee."
                                    android:textColor="@color/grey_70"
                                    android:textSize="@dimen/_13sp" />

                                <com.jobtick.TextView.TextViewMedium
                                    android:layout_marginBottom="@dimen/_5sp"
                                    android:id="@+id/txt_learn_how_level_affects_service_fee"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_10sp"
                                    android:text="More information about levels and service fees."
                                    android:textColor="@color/colorTaskOffer"
                                    android:textSize="@dimen/_11sp" />
                            </LinearLayout>

                        </LinearLayout>
                    </FrameLayout>

