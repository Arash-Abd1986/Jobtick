        app:title="Make An Offer" />-->


    <!--custom tool bar-->


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:clipToPadding="false"
            android:layout_marginBottom="@dimen/_50dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                android:layout_marginTop="@dimen/_10sp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/ivBack"
                    android:padding="@dimen/_5sp"
                    android:layout_width="@dimen/_32sp"
                    android:layout_height="@dimen/_32sp"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_10sp"
                    android:src="@drawable/ic_back" />

                <com.jobtick.TextView.TextViewMedium
                    android:id="@+id/toolbar_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_5sp"
                    android:text="Make an offer"
                    android:textColor="@color/textColor"
                    android:textSize="@dimen/_18sp" />

            </LinearLayout>

            <androidx.core.widget.NestedScrollView
                android:id="@+id/nested_scroll_view"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:overScrollMode="never">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_15sp"
                    android:layout_marginTop="@dimen/_10sp"
                    android:layout_marginEnd="@dimen/_15sp"
                    android:orientation="vertical">


                    <!--Poster Offer Design-->
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sp"
                        android:background="@drawable/bg_poster_offer"
                        android:orientation="horizontal"
                        android:padding="@dimen/_8sp">

                        <com.jobtick.TextView.TextViewMedium
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_5sp"
                            android:layout_marginEnd="@dimen/_5sp"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:text="Poster offer"
                            android:textColor="@color/grey_70"
                            android:textSize="@dimen/_12sp" />

                        <com.jobtick.TextView.TextViewBold
                            android:layout_gravity="center"
                            android:id="@+id/tvOffer"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="$200"
                            android:textColor="@color/colorPrimary"
                            android:textSize="@dimen/_17sp" />

                    </LinearLayout>


                    <!--Edit Offer design enter offer price-->

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="@dimen/_12sp"
                        android:background="@drawable/greay_border_bg"
                        android:visibility="visible">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:padding="@dimen/_8sp">

                            <com.jobtick.TextView.TextViewSemiBold
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_10sp"
                                android:layout_marginTop="@dimen/_5sp"
                                android:text="Your offer"
                                android:textColor="@color/colorPrimary"
                                android:textSize="@dimen/_12sp" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_10sp"
                                android:layout_marginTop="@dimen/_5sp"
                                android:layout_marginBottom="@dimen/_5sp"
                                android:orientation="horizontal">

                                <com.jobtick.EditText.EditTextMedium
                                    android:id="@+id/edt_budget"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:background="@null"
                                    android:hint="5 - 9999 Dollar"
                                    android:inputType="numberDecimal"
                                    android:textColor="@color/textColor"
                                    android:textSize="@dimen/_13sp" />

                                <com.jobtick.TextView.TextViewMedium
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_marginStart="@dimen/_5sp"
                                    android:layout_marginEnd="@dimen/_10sp"
                                    android:text="$"
                                    android:textColor="@color/textColor"
                                    android:textSize="@dimen/_14sp" />

                            </LinearLayout>
                        </LinearLayout>

                    </FrameLayout>

                    <!--Service fees and received free design-->

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="@dimen/_12sp"
                        android:background="@drawable/bg_white_settings"
                        android:visibility="visible">

                        <LinearLayout
                            android:paddingRight="@dimen/_8sp"
                            android:paddingLeft="@dimen/_8sp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_10sp"
                                android:layout_marginTop="@dimen/_5sp"
                                android:orientation="horizontal">

                                <com.jobtick.TextView.TextViewMedium
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:text="Service fee"
                                    android:textColor="@color/grey_70"
                                    android:textSize="@dimen/_13sp" />

                                <com.jobtick.TextView.TextViewSemiBold
                                    android:id="@+id/txt_service_fee"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_10sp"
                                    android:layout_marginTop="@dimen/_5sp"
                                    android:text="$00"
                                    android:textColor="@color/textColor"
                                    android:textSize="@dimen/_17sp" />

                            </LinearLayout>

                            <!--Receive free design-->
                            <LinearLayout
