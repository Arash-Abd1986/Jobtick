                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_10sp"
                                    android:layout_marginTop="@dimen/_5sp"
                                    android:text="$00"
                                    android:textColor="@color/colorPrimary"
                                    android:textSize="@dimen/_21sp" />
                            </LinearLayout>

                        </LinearLayout>

                    </FrameLayout>

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="@dimen/_12sp"
                        android:background="@drawable/bg_white_settings"
                        android:visibility="visible">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:padding="@dimen/_10sp">

                            <!--Level shield design-->
                            <RelativeLayout
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:layout_gravity="center"
                                android:layout_marginTop="@dimen/_10sp">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:src="@drawable/shape_circle_gray_level" />

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:src="@drawable/bg_level_squre_diamond" />

                                <ImageView
                                    android:id="@+id/img_level"
                                    android:layout_width="60dp"
                                    android:layout_height="60dp"
                                    android:layout_centerInParent="true"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"
                                    android:src="@drawable/ic_medal3" />


                            </RelativeLayout>

                            <!--assign level number-->
                            <com.jobtick.TextView.TextViewMedium
